{
  "name": "fudge/dbal-repository",
  "type": "library",
  "description": "A wrapper for Doctrine DBAL to provide a standard repository",
  "keywords": ["dbal", "doctrine", "repository", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Colegate",
      "email": "ben@bencolegate.co.uk"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Fudge\\DBAL\\": ["src/", "tests/"]
    }
  },
  "archive": {
    "exclude": ["!vendor", "tests", "*phpunit.xml"]
  }
}
